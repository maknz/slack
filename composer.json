{
    "name": "maknz/slack",
    "description": "A simple PHP package for sending messages to Slack, with a focus on ease of use and elegant syntax.  Forked from maknz/slack and back-ported to php 5.3.10",
    "keywords": ["slack"],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "maknz",
            "email": "github@mak.geek.nz"
        }
    ],
    "require": {
        "php": ">=5.3.10",
        "guzzle/guzzle": "~3.1",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.9",
        "phpunit/php-file-iterator": "~1.3.1",
        "phpunit/php-text-template": "~1.1.1",
        "phpunit/php-code-coverage": "~1.2.1",
        "phpunit/php-timer": "~1.0.2",
        "phpunit/phpunit-mock-objects": "~1.2.0",
        "symfony/yaml": "~2.1.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Maknz\\Slack\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
